        <ser>use_tcp_hub</ser>
        <ser>tcp_hub_url</ser>
        <ser>tcp_hub_port</ser>
        <ser>tcp_hub_id</ser>
        <ser>tcp_hub_pass</ser>
        <ser>ble_mode</ser>
        <ser>ble_name</ser>
        <ser>ble_pin</ser>
        <ser>ble_service_capacity</ser>
        <ser>ble_chr_descr_capacity</ser>
    </SerOrder>
    <Grouping>
        <group>
            <groupName>General</groupName>
            <subgroup>
                <subgroupName>CAN</subgroupName>
                <subgroupParams>
                    <param>controller_id</param>
                    <param>can_baud_rate</param>
