                </subgroupParams>
            </subgroup>
            <subgroup>
                <subgroupName>WiFi</subgroupName>
                <subgroupParams>
                    <param>ble_name</param>
                    <param>wifi_mode</param>
                    <param>::sep::Station Mode</param>
                    <param>wifi_sta_ssid</param>
                    <param>wifi_sta_key</param>
                    <param>::sep::Access Point Mode</param>
                    <param>wifi_ap_ssid</param>
